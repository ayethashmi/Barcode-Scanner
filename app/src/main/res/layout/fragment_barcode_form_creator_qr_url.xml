<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.views.fragments.barcodeFormCreator.DevAppsBarcodeFormCreatorQrUrlFragmentDevApps">

    <com.devappspros.barcodescanner.presentation.customView.DevAppsCustomTextInputLayout
        android:id="@+id/fragment_barcode_form_creator_qr_url_input_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="@string/qr_code_text_generator_hint_url_input_edit_text"
        app:startIconDrawable="@drawable/baseline_web_24"
        app:startIconTintFocused="?colorPrimary"
        style="@style/AppTheme.TextInputLayoutStyle">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/fragment_barcode_form_creator_qr_url_input_edit_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </com.devappspros.barcodescanner.presentation.customView.DevAppsCustomTextInputLayout>

</FrameLayout>