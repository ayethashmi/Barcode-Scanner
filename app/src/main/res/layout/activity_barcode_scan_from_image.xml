<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".presentation.views.activities.DevAppsBarcodeScanFromImageGalleryActivityDevApps">

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/activity_barcode_scan_from_image_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="?attr/actionBarSize"
        android:background="?android:colorBackground"
        app:titleTextAppearance="@style/AppTheme.ToolBar.TextAppearance"
        app:navigationIcon="@drawable/baseline_arrow_back_24" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/activity_barcode_scan_from_image_toolbar">

        <com.canhub.cropper.CropImageView
            android:id="@+id/activity_barcode_scan_from_image_crop_image_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </FrameLayout>

</RelativeLayout>